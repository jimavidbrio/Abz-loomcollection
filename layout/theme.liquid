<script>
document.addEventListener("DOMContentLoaded", function () {

  function replaceWishlistText(root = document.body) {
    const walker = document.createTreeWalker(root, NodeFilter.SHOW_TEXT);

    let node;
    while (node = walker.nextNode()) {
      if (node.nodeValue.includes("Wishlist")) {
        node.nodeValue = node.nodeValue.replace(/Wishlist/g, "My Projects");
      }
    }
  }

  // Run once
  replaceWishlistText();

  // For dynamically added content
  const observer = new MutationObserver(mutations => {
    for (const mutation of mutations) {
      mutation.addedNodes.forEach(node => {
        if (node.nodeType === 1) {
          replaceWishlistText(node);
        }
      });
    }
  });

  observer.observe(document.body, {
    childList: true,
    subtree: true
  });

});
</script>
<script>
document.addEventListener("DOMContentLoaded", function () {

  function replaceMoveToCart(root = document.body) {
    const walker = document.createTreeWalker(root, NodeFilter.SHOW_TEXT);

    let node;
    while (node = walker.nextNode()) {

      // If text contains "Move to Cart"
      if (node.nodeValue.includes("Move to Cart")) {

        // Remove any + signs (normal or unicode variants)
        node.nodeValue = node.nodeValue
          .replace(/Move to Cart\s*\+?/g, "Move to Cart")
          .replace(/\s{2,}/g, " ") // clean extra spaces
          .trim();
      }
    }
  }

  // Run once on page load
  replaceMoveToCart();

  // Run again for elements loaded later (AJAX/SPA/Apps)
  const observer = new MutationObserver(mutations => {
    for (const mutation of mutations) {
      mutation.addedNodes.forEach(node => {
        if (node.nodeType === 1) {
          replaceMoveToCart(node);
        }
      });
    }
  });

  observer.observe(document.body, {
    childList: true,
    subtree: true
  });

});
</script>

