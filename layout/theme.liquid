        .wpd-processed:has(.wpd-ext-compare-at-price) {
          display: flex;
          flex-direction: column-reverse;
          align-items: baseline;
          .wpd-ext-compare-at-price {
            text-decoration: none !important;
            color: #717171 !important;
            &::before {
              content: 'Retail ';
            }
